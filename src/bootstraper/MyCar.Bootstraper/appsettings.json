{
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    }
  },
  "EmailSettings": {
    "SenderType": "smtp",
    "RetryCountLimit": 5
  },
  "Smtp": {
    "Host": "smtp_server",
    "Issuer": "issuer",
    "IssuerEmail": "issuer@domain.com",
    "Port": 999,
    "Account": "account",
    "Password": "password",
    "SSL": false
  },
  "Auth": {
    "issuerSigningKey": "_________________issuerSigningKey_________________",
    "issuer": "MyCar",
    "validIssuer": "MyCar",
    "validateAudience": false,
    "validateIssuer": true,
    "validateLifetime": true,
    "expiry": "00:10:00",
    "refreshexpiry": "00:30:00",
    "emailConfirmType": "code"
  },
  "Postgress": {
    "connectionString": "Host=host:port;Database=MyCar;Username=username;Password=password"
  },
  "AllowedHost": "localhost"
}
